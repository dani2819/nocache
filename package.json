    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "test": "jest --config test/jest-config.json"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "connect": "^3.7.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
